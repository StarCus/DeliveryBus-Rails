<h1>Listing delivery_men</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @delivery_men.each do |delivery_man| %>
      <tr>
        <td><%= link_to 'Show', delivery_man %></td>
        <td><%= link_to 'Edit', edit_delivery_man_path(delivery_man) %></td>
        <td><%= link_to 'Destroy', delivery_man, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Delivery man', new_delivery_man_path %>
